      <Expression xsi:type="q2:ExpressionTransform">
        <q2:Expression>new(0 as stream, it as time)</q2:Expression>
      </Expression>
      <Expression xsi:type="q2:ExpressionTransform">
        <q2:Expression>new(1 as stream, it as time)</q2:Expression>
      </Expression>
      <Expression xsi:type="Combinator">
