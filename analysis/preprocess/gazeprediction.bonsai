                  <q3:Y>0</q3:Y>
                </q3:Offset>
                <q3:MinArea>100</q3:MinArea>
                <q3:MaxArea xsi:nil="true" />
              </Combinator>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="q3:MorphologicalOperator">
